<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
 
  <bean class="com.springDemo.autowiringxml.Student" name="stud1" autowire="byName">
    <property name="name" value="Utkarsh Pathak"></property>
  </bean>
  <bean class="com.springDemo.autowiringxml.Address" name="address">
  	<property name="city" value="dhule"></property>
  	<property name="state" value="Maharashtra"></property>
  </bean>
</beans>